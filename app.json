{
  "expo": {
    "sdkVersion": "53.0.0",
    "name": "CupCircle",
    "slug": "cup-circle",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/cupcircle_logo.png",
    "scheme": "cupcircle",
    "userInterfaceStyle": "automatic",
    "platforms": ["ios", "android"],
    "newArchEnabled": true,
    "jsEngine": "jsc",
    "extra": {
      "API_URL": "https://cupcircle-api.yourusername.replit.app",
      "eas": {
        "projectId": "c6804025-69e9-411a-9b2a-9eaa2dfb565e"
      }
    },
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "bundleIdentifier": "com.cupcircle.connect",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app requires location access to function properly.",
        "NSLocationAlwaysUsageDescription": "CupCircle uses your approximate location to suggest caf√©s in your neighborhood for coffee chats. Your exact location is never stored or shared.",
        "NSPhotoLibraryUsageDescription": "CupCircle uses your photo library so you can upload a profile picture, helping others recognize you when connecting for coffee chats.",
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["fetch", "remote-notification"]
      },
      "config": {
        "googleMapsApiKey": "AIzaSyDYiKrt8lG2X2HxF4DUqVqIFi4wpGo6Aec"
      },
      "entitlements": {
        "aps-environment": "development"
      },
      "usesAppleSignIn": true
    },
    "android": {
      "package": "com.cupcircle.connect",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/cupcircle_logo.png",
        "backgroundColor": "#F97415"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "READ_MEDIA_IMAGES",
        "CAMERA",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDYiKrt8lG2X2HxF4DUqVqIFi4wpGo6Aec"
        }
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/cupcircle_logo.png",
          "backgroundColor": "#F97415",
          "resizeMode": "contain"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow CupCircle to access your location?",
          "locationWhenInUsePermission": "Allow CupCircle to access your location only while using the app?"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],
      ["expo-apple-authentication"],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#ffffff",
          "defaultChannel": "default"
        }
      ],
      "expo-web-browser",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.145530736366-8o8pc0ttauphf34mkbijf16g3gvtqpc0"
        }
      ],
      "./plugins/withAndroidExported.js"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "nicolebling",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/c6804025-69e9-411a-9b2a-9eaa2dfb565e"
    }
  }
}
